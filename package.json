{
  "name": "cra-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test -o --silent --watchAll=false",
    "test:watch": "react-scripts test -o --watch",
    "test:coverage": "react-scripts test -u --ci --silent --coverage",
    "test:ci": "CI=true react-scripts test -u --silent --verbose --passWithNoTests",
    "predeploy": "yarn build && yarn build-storybook",
    "prettier": "npx prettier --write src/**/*.{ts,tsx,json}",
    "lint": "tsc --noEmit && eslint --fix --ext ts,tsx .",
    "release": "standard-version",
    "release:major": "yarn run release --release-as major --no-verify",
    "release:minor": "yarn run release --release-as minor --no-verify",
    "release:patch": "yarn run release --release-as patch --no-verify",
    "release:alpha": "yarn run release --prerelease alpha --no-verify",
    "release:beta": "yarn run release --prerelease beta --no-verify",
    "storybook": "start-storybook -p 6006 -s public",
    "sitemap": "yarn babel-node src/services/sitemap-generation.js",
    "build:storybook": "build-storybook -o build/storybook",
    "lighthouse": "lighthouse http://localhost:5000 --chrome-flags='--no-sandbox --headless --disable-gpu' --output html --output-path ./lighthouse-report.html --view"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,json}": [
      "pretty-quick --staged"
    ],
    "*.{tsx,ts}": [
      "yarn run lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sentry/react": "^6.5.1",
    "@sentry/tracing": "^6.5.1",
    "axios": "^0.21.1",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.10.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "reinspect": "^1.1.0",
    "typescript": "~4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.9",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-helmet": "^6.1.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-loader": "8.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "26.6.0",
    "jest-transform-stub": "^2.0.0",
    "lighthouse": "^8.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "serve": "^12.0.0",
    "standard-version": "^9.3.0",
    "ts-jest": "^26.5.6"
  }
}
